@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix wgs:   <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix av:    <http://federation.av.tu-berlin.de/about#> .
@prefix omn:   <http://open-multinet.info/ontology/omn#> .
@prefix openstack:    <http://open-multinet.info/ontology/resource/openstack#> .
@prefix openstackvm:  <http://open-multinet.info/ontology/resource/openstackvm#> .

openstack:Openstack
        a                owl:Class ;
        rdfs:label       "Openstack VM Server Type "@en ;
        rdfs:subClassOf  omn:Adapter ;
        omn:implements   openstackvm:OpenstackVM .
        
openstack:images
        a      owl:AsymmetricProperty ,
               owl:IrreflexiveProperty ,
               owl:ObjectProperty ;
        rdfs:domain owl:Thing ;
        rdfs:range owl:Thing ;
        rdfs:label   "The images of the Openstack Instance"@en ;
        rdfs:comment "Defines the images which are available at the Openstack Instance"@en .

av:Openstack-1
        a  openstack:Openstack ;
        omn:partOfGroup av:AV_Smart_Communication_Testbed ;
        rdfs:comment  "An openstack vm server that can handle different VMs."@en ;
        rdfs:label    "A deployed openstack vm server named: Openstack1"@en ;
        wgs:lat       "52.516377" ;
        wgs:long      "13.323732" .

openstackvm:OpenstackVM
        a                  owl:Class ;
        rdfs:label         "OpenstackVM"@en ;
        rdfs:subClassOf    omn:Resource ;
        omn:implementedBy  openstack:Openstack .
        
openstackvm:OpenstackImage
        a                  owl:Class ;
        rdfs:label         "Image"@en .
        
openstackvm:imageid
        a      owl:DatatypeProperty ;
        rdfs:domain  openstackvm:OpenstackImage ;
        rdfs:label   "The ID of an Openstack Image"@en ;
        rdfs:range   xsd:string .

openstackvm:id
        a      owl:DatatypeProperty ;
        rdfs:domain  openstackvm:OpenstackVM ;
        rdfs:label   "The ID of an Openstack VM Instance"@en ;
        rdfs:range   xsd:string .

openstackvm:image
        a      owl:AsymmetricProperty ,
               owl:IrreflexiveProperty ,
               owl:ObjectProperty ;
        rdfs:domain openstackvm:OpenstackVM ;
        rdfs:range owl:Thing ;
        rdfs:label   "The image of the Openstack VM"@en ;
        rdfs:comment "Defines the image used for the Openstack VM"@en .

openstackvm:status
        a      owl:DatatypeProperty ;
        rdfs:domain  openstackvm:OpenstackVM ;
        rdfs:label   "The status of the Openstack VM"@en ;
        rdfs:range   xsd:string .
        
openstackvm:created
        a      owl:DatatypeProperty ;
        rdfs:domain  openstackvm:OpenstackVM ;
        rdfs:label   "The date when the Openstack VM was created"@en ;
        rdfs:range   xsd:string .
        
openstackvm:keypairname
        a      owl:DatatypeProperty ;
        rdfs:domain  openstackvm:OpenstackVM ;
        rdfs:label   "The name of the keypair of the Openstack VM"@en ;
        rdfs:range   xsd:string .
