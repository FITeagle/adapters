fiteagle:Federation rdf:type owl:Class ;

            rdfs:subClassOf fiteagle:Resource .

fiteagle:Adapter rdf:type owl:Class .

fiteagle:FITEAGLE_Testbed rdf:type fiteagle:Federation,
                                        owl:NamedIndividual;
                                         rdfs:label "FITEAGLE_Testbed" ;
                                         rdfs:seeAlso "https://github.com/FITeagle"^^xsd:anyURI ;
                                         wgs:lat       "52.516377" ;
                                         wgs:long      "13.323732" .
              
